{
    "name"             : "laravel/laravel",
    "type"             : "project",
    "description"      : "Dictionary app",
    "keywords"         : [
        "framework",
        "laravel"
    ],
    "license"          : "MIT",
    "require"          : {
        "php": "7.1.* ",
        "ext-json": "*",
        "facade/ignition": "^1.11",
        "fideloper/proxy": "^4.2",
        "illuminate/html": "^5.0",
        "laravel/framework": "5.8.*",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "league/fractal": "^0.17.0",
        "maatwebsite/excel": "^3.1",
        "sentry/sentry-laravel": "1.0.2"
    },
    "require-dev"      : {
        "filp/whoops"                   : "^2.0",
        "fzaninotto/faker"              : "^1.4",
        "nunomaduro/collision"          : "^2.0",
        "phpunit/phpunit"               : "^7.0"
    },
    "config"           : {
        "optimize-autoloader": true,
        "preferred-install"  : "dist",
        "sort-packages"      : true
    },
    "extra"            : {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload"         : {
        "psr-4"   : {
            "App\\"   : "app/",
            "Seeder\\": "database/seeds"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev"     : {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true
}
